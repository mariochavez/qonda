<% @columns_distribution="" %>

<div id="photoset">
  <div id="info">
    <strong>Tag <%= @tag %></strong>
    <p><%= @user.name unless @user.nil? %></p>
  </div>
  <div>
  <% unless @photos.blank? %>
    <% for photo in @photos -%><%= link_to image_tag(photo.image.url(:tiny)), picto_photo_url(photo), {:title => photo.title} -%><% end -%>
  <% else %>
    No photos with this tag yet.
  <% end %>
  </div>
</div>