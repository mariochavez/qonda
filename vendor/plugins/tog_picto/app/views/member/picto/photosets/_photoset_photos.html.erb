<ul>
  <% unless photos_in_the_set.blank? %>
    <% for photo in photos_in_the_set %>
      <%= image_tag photo.image.url(:tiny), :style=>"cursor:move", :class => "photo_in_set",:id => "photo_in_set_#{photo.id}"  %>
      <%= draggable_element "photo_in_set_#{photo.id}", :revert => true %>
    <% end %>
  <% else %>
    <%= I18n.t("tog_picto.photoset_photos.no_photos_yet") %>.
  <% end %>
</ul>
